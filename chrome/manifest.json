{
  "name": "Throw URL",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Throw URL at another computer",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Throw URL",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "http://*/",
    "tabs"
  ]
}
